     ]
    }
   ],
   "source": [
    "from tqdm import tqdm\n",
    "\n",
    "LIMIT_OF_FILES = min(len(files), 12)\n",
    "games = []\n",
    "\n",
    "i = 1\n",
    "\n",
    "for file in tqdm(files):\n",
    "    games.extend(load_pgn(f\"Database/{file}\"))\n",
    "    if (i >= LIMIT_OF_FILES):\n",
    "        break\n",
    "\n",
    "    i+=1\n",
    "\n"
   ]
